<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="ThMapper">

	<select id="seatinfo" resultType="Thboard">
		SELECT * FROM seat
		WHERE
		id=#{id}
	</select>

	 <select id="seatList" resultType="Thboard">
		SELECT * FROM seat
		WHERE
		thnum= #{thnum}
	</select> 
</mapper>
