<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="MuseumMapper" >

	<select id="museumList" resultType="Museum">
		SELECT * FROM total_ent WHERE category = '1'
	</select>
	<select id="museumDetail" resultType="Museum">
		SELECT * FROM total_ent WHERE tseq = #{tseq}
	</select>

</mapper>
