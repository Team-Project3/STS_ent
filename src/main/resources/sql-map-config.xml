	</properties>
	<typeAliases>
		<typeAlias type="com.ezen.biz.dto.MemberVO" alias="Member"/>
		<typeAlias type="com.ezen.biz.dto.AdminVO" alias="Admin"/>
		<typeAlias type="com.ezen.biz.dto.BookingVO" alias="Booking"/>
